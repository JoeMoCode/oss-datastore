{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadAccess",
            "Effect": "Allow",
            "Principal": {
                "AWS": [ "arn:aws:iam::123456789012:root" ]
            },
            "Action": "s3:GetObject",
            "Resource": [ "arn:aws:s3:::source-bucket-name/*" ]
        },
        {
            "Sid": "ListAccess",
            "Effect": "Allow",
            "Principal": {
                "AWS": [ "arn:aws:iam::123456789012:root" ]
            },
            "Action": "s3:ListBucket",
            "Resource": [ "arn:aws:s3:::source-bucket-name" ]
        }
    ]
}